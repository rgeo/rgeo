#!/usr/bin/env ruby3.2
#
# This file was generated by RubyGems.
#
# The application 'ruby_memcheck' is installed as part of a gem, and
# this file is here to facilitate running it.
#

require 'rubygems'

Gem.use_gemdeps

version = ">= 0.a"

str = ARGV.first
if str
  str = str.b[/\A_(.*)_\z/, 1]
  if str and Gem::Version.correct?(str)
    version = str
    ARGV.shift
  end
end

if Gem.respond_to?(:activate_bin_path)
load Gem.activate_bin_path('ruby_memcheck', 'ruby_memcheck', version)
else
gem "ruby_memcheck", version
load Gem.bin_path("ruby_memcheck", "ruby_memcheck", version)
end
